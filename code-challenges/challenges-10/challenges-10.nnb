            "source": [
                "function compareTriplets(a, b) {\n    let respostas = [0,0];\n    for (let i = 0; i < 3 ; i++) {\n        if (a[i] > b[i] ) {\n            // console.log(alice)\n            respostas[0]++ ;\n        } else if (a[i] < b[i] ) {\n            respostas[1]++ ;\n        }\n    }\n    return respostas;\n}"
